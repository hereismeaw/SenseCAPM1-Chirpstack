                            "nb_chan": 8,
                            "nb_scan": 2000,
                            "pace_s": 10
                                                    },
                    "lbt":                                          /* AS923 LBT Configuration SX1303 Module ONLY! Ex. Seeed WM1303, RAK5146 */
