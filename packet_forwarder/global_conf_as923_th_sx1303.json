                    "freq": 922300000,                                  /* Main Frequency in hz */
                    "rssi_offset": -215.4,
                    "rssi_tcomp": {"coeff_a": 0, "coeff_b": 0, "coeff_c": 20.41, "coeff_d": 2162.56, "coeff_e": 0},
