                    "tx_freq_max": 928000000,                          /* Maximum Tx Frequency in hz for Southnern IoT we Cover AS1 AS2 AS3 AS4 Freqency Plan */
                    "tx_gain_lut":
                                        /* --- RF Power Table --- */
                        [
                              {"rf_power": 12, "pa_gain": 1, "pwr_idx": 4},
                              {"rf_power": 13, "pa_gain": 1, "pwr_idx": 5},
                              {"rf_power": 14, "pa_gain": 1, "pwr_idx": 6},
